<li>
  <%= link_to user.name, user %>
  <div class="delete_button">
    <%= button_to t(".delete"), admin_user_path(user), method: :delete, data: {confirm: t(".confirm")} %>
  </div>
  <p>
    <%= t ".role" %>
    <%= user.role %>
  </p>
  <p>
    <%= t ".created_at" %>
    <%= user.created_at.strftime("%FT%T") %>
  </p>
</li>
