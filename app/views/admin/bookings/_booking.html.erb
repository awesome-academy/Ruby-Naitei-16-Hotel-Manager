<tr id="booking-<%= booking.id %>">
  <td><%= booking.id %></td>
  <td><%= link_to booking.room.room_number, "#" %></td>
  <td><%= link_to booking.room.room_type.name, "#" %></td>
  <td><%= booking.updated_at.to_date %></td>
  <td><%= booking.checkin.to_date %></td>
  <td><%= booking.checkout.to_date %></td>
  <td><%= booking.is_checkout? %></td>
  <td><%= total_calculate booking %> <%= t "currency" %></td>
  <td>
    <%= link_to edit_admin_booking_path(booking), class: "btn btn-default" do %>
      <span class="glyphicon glyphicon-pencil"></span>
    <% end %>
  </td>
  <td>
    <%= link_to admin_booking_path(booking), method: :delete,
                  data: {confirm: t(".confirm")}, class: "btn btn-danger" do %>
      <span class="glyphicon glyphicon-trash"></span>
    <% end %>
    </td>
</tr>
